# Variables d'environnement pour Capture de Drapeau

# MongoDB Atlas Connection String
# Remplacez <password> par votre mot de passe MongoDB
# Remplacez <cluster-url> par l'URL de votre cluster
MONGODB_URI=mongodb+srv://capture-admin:<password>@<cluster-url>/capture-drapeau?retryWrites=true&w=majority

# Port du serveur (optionnel)
PORT=3000
